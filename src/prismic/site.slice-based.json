    "about_video_1": {
      "type": "Link",
      "config": {
        "select": "media",
        "label": "About Video 1"
      }
    },
    "recognition": {
      "type": "Group",
      "config": {
        "fields": {
          "link": {
            "type": "Link",
            "config": {
              "label": "Link",
              "allowTargetBlank": true
            }
          },
          "title": {
            "type": "StructuredText",
            "config": {
              "multi": "heading3, hyperlink",
              "allowTargetBlank": true,
              "label": "Title"
            }
          }
        },
        "label": "Recognition"
      }
    },
    "body": {
      "type": "Slices",
      "fieldset": "Slice zone",
      "config": {
        "choices": {
          "global_block": {
            "type": "Slice",
            "fieldset": "Global Block",
            "description": "Add Global Element",
            "icon": "face",
            "display": "list",
            "non-repeat": {
              "block_name": {
                "type": "Select",
                "config": {
                  "options": ["Logos"],
                  "default_value": "Logos",
                  "label": "Block Name"
                }
              }
            },
            "repeat": {}
          },
          "text_block": {
            "type": "Slice",
            "fieldset": "Text Block",
            "description": "Add a large block of text",
            "icon": "face",
            "display": "list",
            "non-repeat": {
              "text": {
                "type": "StructuredText",
                "config": {
                  "multi": "heading2, heading3, heading4, heading5, heading6, strong, em",
                  "label": "Text"
                }
              }
            },
            "repeat": {}
          },
          "image_block": {
            "type": "Slice",
            "fieldset": "Image Block",
            "description": "Add one or more images",
            "icon": "image",
            "display": "list",
            "non-repeat": {},
            "repeat": {
              "img": {
                "type": "Image",
                "config": {
                  "constraint": {
                    "width": 1200
                  },
                  "thumbnails": [
                    {
                      "name": "md",
                      "width": 1200,
                      "height": null
                    },
                    {
                      "name": "lg",
                      "width": 2400,
                      "height": null
                    }
                  ],
                  "label": "Image"
                }
              },
              "layout": {
                "type": "Select",
                "config": {
                  "options": ["Full-Width", "Half-Width"],
                  "default_value": "Full-Width",
                  "label": "Layout"
                }
              },
              "desc": {
                "type": "StructuredText",
                "config": {
                  "single": "paragraph, em",
                  "label": "Description"
                }
              }
            }
          },
          "repeatable_text": {
            "type": "Slice",
            "fieldset": "Collapsable Text",
            "description": "Collapsable Text",
            "icon": "face",
            "display": "list",
            "non-repeat": {
              "columns": {
                "type": "Select",
                "config": {
                  "options": ["Two Column", "One Column"],
                  "default_value": "Two Column",
                  "label": "Columns"
                }
              }
            },
            "repeat": {}
          }
        }
      }
    }
  },
